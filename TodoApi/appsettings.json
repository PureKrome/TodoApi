{
    "Serilog": {
        "Enrich": [ "FromLogContext", "WithExceptionDetails" ],
        "Properties": {
            "Application": "TodoApi"
        },
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information"
            }
        }
    },

    "AllowedHosts": "*"
}
